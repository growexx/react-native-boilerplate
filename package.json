{
  "name": "RNBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android:dev": "react-native run-android --variant=developmentDebug",
    "android:stg": "react-native run-android --variant=stagingDebug",
    "android:prod": "react-native run-android --variant=productionDebug",
    "ios:dev": "react-native run-ios --scheme 'DevRNBoilerplate'",
    "ios:stg": "react-native run-ios --scheme 'StagRNBoilerplate'",
    "ios:prod": "react-native run-ios --scheme 'RNBoilerplate'",
    "codepush:android:dev": "appcenter codepush release-react -a prince.dholiya/ReactNativeBoilerplate-Android -d Development",
    "codepush:android:stg": "appcenter codepush release-react -a prince.dholiya/ReactNativeBoilerplate-Android -d Staging",
    "codepush:android:prod": "appcenter codepush release-react -a prince.dholiya/ReactNativeBoilerplate-Android -d Production",
    "codepush:ios:dev": "appcenter codepush release-react -a prince.dholiya/ReactNativeBoilerplate-iOS -d Development",
    "codepush:ios:stg": "appcenter codepush release-react -a prince.dholiya/ReactNativeBoilerplate-iOS -d Staging",
    "codepush:ios:prod": "appcenter codepush release-react -a prince.dholiya/ReactNativeBoilerplate-iOS -d Production",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^9.3.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.4.0",
    "moment": "^2.29.4",
    "npm-check-updates": "^16.10.9",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-code-push": "^8.0.1",
    "react-native-codegen": "^0.71.5",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.6.0",
    "react-native-fbsdk": "^3.0.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-i18n": "^2.0.15",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.2",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/runtime": "^7.21.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "babel-jest": "^29.5.0",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "^0.76.3",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native-community/google-signin|@react-native|react-native|@react-navigation|reactxp-webview|react-pose-core|animated-pose|@react-native-community/async-storage|@invertase/react-native-apple-authentication)"
    ],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./jest.mock.js"
    ]
  }
}
